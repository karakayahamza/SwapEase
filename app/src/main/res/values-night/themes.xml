<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Theme.SwapEase.Dark" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/darkPrimaryColor</item>
        <item name="colorPrimaryVariant">@color/darkPrimaryVariantColor</item>
        <item name="colorOnPrimary">@color/lightTextColor</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your dark mode theme here. -->
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

</resources>